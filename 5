<body>
    <script>
        let rectangle = {
            width: 10,
            height: 5,
            color: "blue",
        };
        for (let key in rectangle) {
            console.log(`${key}: ${rectangle[key]}`);
        }
        let result = rectangle.width * rectangle.height;
        console.log("Diện tích hình chữ nhật thông qua 2 thuộc tính width và height là: " + result);
    </script>
</body>
